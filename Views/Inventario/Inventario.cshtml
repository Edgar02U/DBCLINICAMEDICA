@model IEnumerable<Entidad.INVENTARIO>
@{
    ViewBag.Title = "Inventario";
}
@{ 
    
    Entidad.MEDICINA dbmed = new Entidad.MEDICINA();
}
<br />
<br />
<br />
<section class="content">
    <div class="box box-primary">
        <div class="box-header with-border">
            <h3 class="box-title">Inventario <a title="Nuevo" class="btn btn-default btn-sm checkbox-toggle" href="Create"><i class="fa fa-file-o"> Nuevo </i></a></h3>
        </div><!-- /.box-header -->
        <div class="box-body no-padding">
            <div class="table-responsive mailbox-messages">
                <table id="tbInventario" class="table table-striped table-bordered dt-responsive display" cellspacing="0" width="100%">
                    <tr>
                        <th>@Html.DisplayNameFor(model => model.ID_TIPO_INVENTARIO)</th>

                        <th>@Html.DisplayNameFor(model => model.ID_MEDICINA)</th>

                        <th>@Html.DisplayNameFor(model => model.CANTIDAD)</th>

                        <th>@Html.DisplayNameFor(model => model.ID_SUCURSAL)</th>

                    </tr>
                    <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <td>@Html.DisplayFor(modeltem => item.ID_TIPO_INVENTARIO)</td>

                                <td>
                                    @Html.DisplayFor(modeltem => item.ID_MEDICINA)
                                </td>

                                <td>@Html.DisplayFor(modeltem => item.CANTIDAD)</td>

                                <td>@Html.DisplayFor(modeltem => item.ID_SUCURSAL)</td>

                                <td>
                                    <button title="Editar" onclick="location.href='@Url.Action("Edit", new { id = item.ID_TIPO_INVENTARIO })';return false;" class="btn btn-default btn-sm"><i class="btn btn-primary btn-sd">Editar</i></button>
                                    <button title="Eliminar" onclick="location.href='@Url.Action("Delete", new { id = item.ID_TIPO_INVENTARIO})'; return false;" class="btn btn-default btn-sm"><i class="btn btn-primary btn-sd">Eliminar</i></button>
                                </td>
                            </tr>
                        }
                    </tbody>
                </table><!-- /.table -->
            </div><!-- /.mail-box-messages -->
        </div><!-- /.box-body -->
    </div><!-- /. box -->


</section>


