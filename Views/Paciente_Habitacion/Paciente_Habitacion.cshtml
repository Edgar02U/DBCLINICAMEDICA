@model IEnumerable<Entidad.PACIENTE_HABITACION>
@{
    ViewBag.Title = "Paciente Habitacion";
}
<br />
<br />
<br />
<section class="content">
    <div class="row">
        <div class="col-md-12">
        </div><!-- /.col -->
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Paciente Habitacion <a title="Nuevo" class="btn btn-default btn-sm checkbox-toggle" href="Create"><i class="fa fa-file-o">Nuevo  </i></a></h3>
                </div><!-- /.box-header -->
                <div class="box-body no-padding">
                    <div class="table-responsive mailbox-messages">
                        <table id="tblmedicina" class="table table-striped table-bordered dt-responsive display" cellspacing="0" width="100%">
                            <tr>
                                <th>@Html.DisplayNameFor(model => model.ID_PACIENTE_HABITACION)</th>

                                <th>@Html.DisplayNameFor(model => model.ID_PACIENTE)</th>

                                <th>@Html.DisplayNameFor(model => model.ID_HABITACION)</th>

                                <th>@Html.DisplayNameFor(model => model.Fecha_Estancia)</th>
                            </tr>

                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@Html.DisplayFor(modeltem => item.ID_PACIENTE_HABITACION)</td>

                                    <td>@Html.DisplayFor(modeltem => item.ID_PACIENTE)</td>

                                    <td>@Html.DisplayFor(modeltem => item.ID_HABITACION)</td>

                                    <td>@Html.DisplayFor(modeltem => item.Fecha_Estancia)</td>
                                    <td>
                                        <button title="Editar" onclick="location.href='@Url.Action("Edit", new { id = item.ID_PACIENTE_HABITACION })';return false;" class="btn btn-default btn-sm"><i class="fa fa-pencil">Editar</i></button>
                                        <button title="Eliminar" onclick="location.href='@Url.Action("Delete", new { id = item.ID_PACIENTE_HABITACION})'; return false;" class="btn btn-default btn-sm"><i class="fa fa-trash-o">Eliminar</i></button>
                                    </td>
                                </tr>
                            }

                        </table><!-- /.table -->
                    </div><!-- /.mail-box-messages -->
                </div><!-- /.box-body -->
            </div><!-- /. box -->
        </div><!-- /.col -->
    </div><!-- /.row -->
</section>

