@model IEnumerable<Entidad.USUARIO>
@{
    ViewBag.Title = "Usuario";
}
<br />
<br />
<br />
<section class="content">
    <div class="row">
        <div class="col-md-12">
        </div><!-- /.col -->
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">
                        Usuario <a title="Nuevo" class="btn btn-default btn-sm checkbox-toggle" href="Create"><i class="fa fa-file-o"> Nuevo </i></a>
                                <a title="Eliminar" class="btn btn-default btn-sm checkbox-toggle" href="Delete"><i class="fa fa-file-o"> Eliminar </i></a>
                    </h3>
                </div><!-- /.box-header -->
                <div class="box-body no-padding">
                    <div class="table-responsive mailbox-messages">
                        <table id="tblusuario" class="table table-striped table-bordered dt-responsive display" cellspacing="0" width="100%">
                            <tr>
                                <th>@Html.DisplayNameFor(model => model.ID_USUARIO)</th>

                                <th>@Html.DisplayNameFor(model => model.PRIMER_NOMBRE)</th>

                                <th>@Html.DisplayNameFor(model => model.SEGUNDO_NOMBRE)</th>

                                <th>@Html.DisplayNameFor(model => model.PRIMER_APELLIDO)</th>

                                <th>@Html.DisplayNameFor(model => model.SEGUNDO_APELLIDO)</th>

                                <th>@Html.DisplayNameFor(model => model.USERC)</th>

                                <th>@Html.DisplayNameFor(model => model.EDAD)</th>

                                <th>@Html.DisplayNameFor(model => model.FECHA_CREACION)</th>

                                <th>@Html.DisplayNameFor(model => model.ESTADO)</th>

                                <th>@Html.DisplayNameFor(model => model.PUESTO)</th>

                                <th>@Html.DisplayNameFor(model => model.GENERO)</th>

                                <th>@Html.DisplayNameFor(model => model.ID_SUCURSAL)</th>


                            </tr>

                            @foreach (var item in Model)
                            {
                                <tr>
                                    <td>@Html.DisplayFor(modeltem => item.ID_USUARIO)</td>

                                    <td>@Html.DisplayFor(modeltem => item.PRIMER_NOMBRE)</td>

                                    <td>@Html.DisplayFor(modeltem => item.SEGUNDO_NOMBRE)</td>

                                    <td>@Html.DisplayFor(modeltem => item.PRIMER_APELLIDO)</td>

                                    <td>@Html.DisplayFor(modeltem => item.SEGUNDO_APELLIDO)</td>

                                    <td>@Html.DisplayFor(modeltem => item.USERC)</td>

                                    <td>@Html.DisplayFor(modeltem => item.EDAD)</td>

                                    <td>@Html.DisplayFor(modeltem => item.FECHA_CREACION)</td>

                                    <td>@Html.DisplayFor(modeltem => item.ESTADO)</td>

                                    <td>@Html.DisplayFor(modeltem => item.PUESTO)</td>

                                    <td>@Html.DisplayFor(modeltem => item.GENERO)</td>

                                    <td>@Html.DisplayFor(modeltem => item.ID_SUCURSAL)</td>
                                </tr>
                            }

                        </table><!-- /.table -->
                    </div><!-- /.mail-box-messages -->
                </div><!-- /.box-body -->
            </div><!-- /. box -->
        </div><!-- /.col -->
    </div><!-- /.row -->
</section>

