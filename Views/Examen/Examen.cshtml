@model IEnumerable<Entidad.EXAMEN>
@{
    ViewBag.Title = "Examen";
}
<br />
<br />
<br />
<section class="content">
    <div class="row">
        <div class="col-md-12">
        </div><!-- /.col -->
        <div class="col-md-12">
            <div class="box box-primary">
                <div class="box-header with-border">
                    <h3 class="box-title">Examen <a title="Nuevo" class="btn btn-default btn-sm checkbox-toggle" href="Create"><i class="fa fa-file-o"> Nuevo </i></a></h3>
                </div><!-- /.box-header -->
                <div class="box-body no-padding">
                    <div class="table-responsive mailbox-messages">
                        <table id="tblexamen" class="table table-striped table-bordered dt-responsive display" cellspacing="0" width="100%">
                            <tr>
                                <th>@Html.DisplayNameFor(model => model.ID_EXAMEN)</th>

                                <th>@Html.DisplayNameFor(model => model.NOMBRE_EXAMEN)</th>

                                <th>@Html.DisplayNameFor(model => model.PRECIO)</th>

                                <th>@Html.DisplayNameFor(model => model.RECOMENDACION)</th>

                                <th>@Html.DisplayNameFor(model => model.DESCRIPCION)</th>

                                <th>@Html.DisplayNameFor(model => model.TRATAMIENTO)</th>
                            </tr>
                            <tbody>
                                @foreach (var item in Model)
                                {
                                    <tr>
                                        <td>@Html.DisplayFor(modeltem => item.ID_EXAMEN)</td>

                                        <td>@Html.DisplayFor(modeltem => item.NOMBRE_EXAMEN)</td>

                                        <td>@Html.DisplayFor(modeltem => item.PRECIO)</td>

                                        <td>@Html.DisplayFor(modeltem => item.RECOMENDACION)</td>

                                        <td>@Html.DisplayFor(modeltem => item.DESCRIPCION)</td>

                                        <td>@Html.DisplayFor(modeltem => item.TRATAMIENTO)</td>
                                        <td>
                                            <button title="Editar" onclick="location.href='@Url.Action("Edit", new { id = item.ID_EXAMEN })';return false;" class="btn btn-default btn-sm"><i class="fa fa-pencil">Editar</i></button>
                                            <button title="Eliminar" onclick="location.href='@Url.Action("Delete", new { id = item.ID_EXAMEN})'; return false;" class="btn btn-default btn-sm"><i class="fa fa-trash-o">Eliminar</i></button>
                                        </td>
                                    </tr>
                                }
                            </tbody>
                        </table><!-- /.table -->
                    </div><!-- /.mail-box-messages -->
                </div><!-- /.box-body -->
            </div><!-- /. box -->
        </div><!-- /.col -->
    </div><!-- /.row -->
</section>

